msgstr "Gallery konfigurasjonsveiviser: Steg %d"

#: ../setup/check.inc:31
msgid "This is the Gallery system check page."
msgstr "Dette er Gallerys systemsjekk-side."

#: ../setup/check.inc:32
msgid "This page will validate your installation to make sure that you have all the necessary components and permissions on this system and that everything is configured reasonably."
msgstr "Denne siden vil kontrollere installasjonen for &aring; bekrefte at du har alle n&oslash;dvendige komponenter og tillatelser p&aring; dette systemet, og at alt er konfigurert riktig."

#: ../setup/check.inc:37 ../setup/diagnostics.php:30
msgid "Gallery Diagnostics Page"
msgstr "Gallery diagnose-side"

#: ../setup/check.inc:38
msgid "Gallery Help Page"
msgstr "Gallery hjelpeside"

#: ../setup/check.inc:42
#, php-format
msgid "Having problems?  Try the %s, %s and %s."
msgstr "Problemer?  Pr&oslash;v %s, %s og %s."

#: ../setup/check.inc:43 ../setup/confirm.inc:36
msgid "documentation"
msgstr "dokumentasjon"

#: ../setup/check.inc:45
#, php-format
msgid "Having problems?  Try the %s and %s."
msgstr "Problemer?  Pr&oslash;v %s og %s."

#: ../setup/check.inc:68
msgid "Success"
msgstr "Suksess"

#: ../setup/check.inc:80 ../setup/check.inc:94
