#: ../setup/check.inc:68
msgid "Success"
msgstr "Успешно"

#
#: ../setup/check.inc:80 ../setup/check.inc:94
msgid "Serious Warning"
msgstr "Серьезное предупреждние"

#
#: ../setup/check.inc:83 ../setup/check.inc:123 ../setup/check_mail.php:75
#: ../setup/config_data.inc:998 ../setup/config_data.inc:1004
msgid "Warning"
msgstr "Предупреждение"

#
