#: ../includes/setup/constants.inc:244 ../setup/config_data.inc:407
msgid "ImageMagick Settings"
msgstr "Configurações do ImageMagick"

#: ../includes/setup/constants.inc:250 ../setup/config_data.inc:412
msgid "Path to ImageMagick"
msgstr "Caminho para ImageMagick"

#: ../includes/setup/constants.inc:251 ../setup/config_data.inc:413
